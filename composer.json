{
    "name": "spryker-demo/merchant-onboarding",
    "type": "library",
    "description": "Merchant Onboarding module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/merchant": "*",
        "spryker/state-machine": "*",
        "spryker/kernel": "*"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "config": {
        "sort-packages": true
    }
}
