{
    "name": "spryker-demo/merchant-onboarding",
    "type": "library",
    "description": "Merchant Onboarding module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/kernel": "^3.70.0",
        "spryker/merchant": "^3.5.1",
        "spryker/merchant-extension": "^1.2.0",
        "spryker/state-machine": "^2.16.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "^0.17.18"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "config": {
        "sort-packages": true
    }
}
